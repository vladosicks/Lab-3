using System;
using System.Collections.Generic;

// Базовий клас "Комп'ютер"
class Компютер
{
    public string IPАдреса { get; set; }
    public int Потужність { get; set; }
    public string ТипОС { get; set; }
}

// Спадкоємні класи
class Сервер : Компютер, IConnectable
{
    public int КількістьПідключень { get; set; }

    public void Підключити(Компютер компютер)
    {
        // Реалізація підключення сервера до інших комп'ютерів
    }

    public void Відключити(Компютер компютер)
    {
        // Реалізація відключення сервера від інших комп'ютерів
    }
}

class РобочаСтанція : Компютер, IConnectable
{
    public void Підключити(Компютер компютер)
    {
        // Реалізація підключення робочої станції до інших комп'ютерів
    }

    public void Відключити(Компютер компютер)
    {
        // Реалізація відключення робочої станції від інших комп'ютерів
    }
}

class Маршрутизатор : Компютер, IConnectable
{
    public void Підключити(Компютер компютер)
    {
        // Реалізація підключення маршрутизатора до інших комп'ютерів
    }

    public void Відключити(Компютер компютер)
    {
        // Реалізація відключення маршрутизатора від інших комп'ютерів
    }
}

// Інтерфейс для з'єднання
interface IConnectable
{
    void Підключити(Компютер компютер);
    void Відключити(Компютер компютер);
}

// Клас "Мережа"
class Мережа
{
    private List<Компютер> комп'ютери;

    public Мережа()
    {
        комп'ютери = new List<Компютер>();
    }

    public void ДодатиКомпютер(Компютер компютер)
    {
        комп'ютери.Add(компютер);
    }

    public void ПередатиДані(Комп'ютер джерело, Компютер приймач)
    {
    }

    public void Відключити(Компютер комп'ютер)
    {       
    }
}
